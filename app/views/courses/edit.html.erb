<div class="page-header">
	<h1>Edit Course</h1>
</div>

<%= form_for [@generation, @course], html: { class: "form-horizontal generation" } do |f| %>
	<%= render 'form', f: f %>
	<div class="form-group">
		<div class="col-lg-offset-2 col-lg-10">
			<%= f.submit "Update", class: 'btn btn-primary' %>
			<%= link_to "Cancel", generation_courses_path(@generation), class: 'btn btn-default' %>
    	</div>
	</div>
<% end %>